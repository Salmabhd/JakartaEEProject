<web-app xmlns="http://jakarta.ee/xml/ns/jakartaee" version="5.0">
    <servlet>
        <servlet-name>liste</servlet-name>
        <servlet-class>com.example.servlet.ListeEtudiantsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>liste</servlet-name>
        <url-pattern>/etudiants</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>enseignants</servlet-name>
        <servlet-class>com.example.servlet.ListeEnseignantsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>enseignants</servlet-name>
        <url-pattern>/enseignants</url-pattern>
    </servlet-mapping>
<servlet>
   <servlet-name>departements</servlet-name>
   <servlet-class>com.example.servlet.ListeDepartementsServlet</servlet-class>
</servlet>

<servlet-mapping>
   <servlet-name>departements</servlet-name>
   <url-pattern>/departements</url-pattern>
</servlet-mapping>


  <servlet>
        <servlet-name>salle</servlet-name>
        <servlet-class>com.example.servlet.ListeSalleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>salle</servlet-name>
        <url-pattern>/salle</url-pattern>
    </servlet-mapping>

     <servlet>
        <servlet-name>listeFilieres</servlet-name>
        <servlet-class>com.example.servlet.ListeFiliereServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>filieres</servlet-name>
        <url-pattern>/filieres</url-pattern>
    </servlet-mapping>

</web-app>  